{
	"$schema": "https://biomejs.dev/schemas/1.8.2/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"correctness": {
				"noUnusedImports": "warn"
			},
			"style": {
				"noNonNullAssertion": "off"
			}
		}
	},
	"formatter": { "enabled": true }
}
